@{
    Layout = null;
}

<form id="form2" runat="server">
    <table height="100%" width="100%" background="~/Images/images/index_r25_c1.jpg">
        <tr>
            <td width="200px" align="center" valign="top">
                <table width="100%"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            <img src="~/Images/images/index_r3_c1.jpg" width="200px" height="41px" />
                        </td>
                    </tr>
                </table>
                <table width="50%" height="10px" border="0" align="center" cellpadding="0" cellspacing="0">
                    <tr>
                        <td></td>
                    </tr>
                </table>
                <table runat="server" id="bbck" width="50%"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("AccountSummary")*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbManage" width="50%"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("SubGroupManage")*@
                        </td>
                    </tr>
                </table>
                <table width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td width="20px" class="font1" align="left" valign="top">
                            @*tree view*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbPlayList" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("PlayList")*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbPlayMoney" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("PlayMoney")*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbLoginList" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection(BettingInfo)*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbGame" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("Game")*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="tbChangePwd" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("ChangePwd")*@
                        </td>
                    </tr>
                </table>
                <table runat="server" id="ListNotice" width="168px"   cellspacing="0" cellpadding="0">
                    <tr>
                        <td onmouseover='this.className="mouseover"' onmouseout='this.className="mouseout"'>
                            @*@RenderSection("ListNotice")*@
                        </td>
                    </tr>
                </table>
                <br />
            </td>
        </tr>
    </table>
</form>

